pipeline {
    agent any

    stages {
		stage('git checkout') {
			steps {
				checkout([
					$class: 'GitSCM',
					branches: [[name: '*/master']],
					extensions: [
					],
					userRemoteConfigs: [
						[
						    //https git clone
                            //url: 'https://github.com/ivanlevsky/stand-alone-complex-tomato.git'

                            //ssh git clone
                            credentialsId: 'jenkins-ssh-key',
                            url: 'git@github.com:ivanlevsky/stand-alone-complex-tomato.git'
						]
					]
				])
			}

		}

    }
}